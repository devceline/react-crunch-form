{
  "name": "react-crunch-form",
  "version": "0.0.2",
  "description": "A library that enables you to quickly crunch out a form that performs as you would expect",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/shaheensarafa/react-crunch-form",
  "author": "Shaheen Sarafa",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "license": "MIT",
  "private": false,
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "react": ">=17.0.1",
    "react-dom": ">=17.01"
  }
}
